	<parameter name="invoiceId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
            mrl.mediation_record_id,
            mrl.description,
            mrl.event_date,
            mrl.amount
        from
            mediation_record_line mrl
            join order_line l on l.id = mrl.order_line_id
            join order_process p on p.order_id = l.order_id
        where
            p.is_review = 0
            and p.invoice_id = $P{invoiceId};]]>
	</queryString>

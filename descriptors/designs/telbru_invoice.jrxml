	<queryString>
		<![CDATA[select
            l.amount,
            tm.type_id
        from
            invoice i
            join invoice_line l on l.invoice_id = i.id
            join item_type_map tm on tm.item_id = l.item_id
        where
            l.is_percentage = 0
            and i.id = $P{invoiceId}]]>
	</queryString>

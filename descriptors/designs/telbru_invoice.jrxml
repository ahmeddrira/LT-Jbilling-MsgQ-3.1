	<field name="type_id" class="java.lang.Integer"/>
	<variable name="rental" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{type_id}.equals(Integer.valueOf(2500)) ? $F{amount} : new BigDecimal("0.00")]]></variableExpression>

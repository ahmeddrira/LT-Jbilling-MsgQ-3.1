<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="blinkmind_sub_onetime" pageWidth="540" pageHeight="802" columnWidth="540" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="entityId" class="java.lang.Integer"/>
	<parameter name="startDate" class="java.util.Date"/>
	<parameter name="endDate" class="java.util.Date"/>
	<queryString>
		<![CDATA[select
          sum(i.total) as total,
          count(i.id) as count,
          to_char(i.create_datetime, 'WW') as week_of_year
        from
          invoice i
        where
          i.deleted = 0
          and i.is_review = 0
          and i.create_datetime >= $P{startDate}
          and i.create_datetime <= $P{endDate}
        group by week_of_year
        order by week_of_year]]>
	</queryString>
	<field name="total" class="java.math.BigDecimal"/>
	<field name="count" class="java.lang.Integer"/>
	<field name="week_of_year" class="java.lang.Integer"/>
	<variable name="subtotal" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{total}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal("0.00")]]></initialValueExpression>
	</variable>
	<columnHeader>

					<pen lineWidth="2.0"/>
				</graphicElement>
			</line>
		</band>
	</columnHeader>
	<detail>
		<band height="27">
			<textField>
				<reportElement x="0" y="0" width="120" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{count}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement x="120" y="0" width="100" height="20"/>
				<textElement verticalAlignment="Middle"/>
				<textFieldExpression class="java.lang.Integer"><![CDATA[$F{week_of_year}]]></textFieldExpression>
			</textField>
			<textField pattern="Â¤ #,##0.00">
				<reportElement x="407" y="0" width="120" height="20"/>
